ter justify-space-between-md align-center\"\n        onSubmit={handleFormSubmit}\n      >\n        <div className=\"col-12\">\n          <textarea\n            name=\"nameText\"\n            placeholder=\"Here's a new thought...\"\n            value={formState.nameText}\n            className=\"form-input w-100\"\n            onChange={handleChange}\n          ></textarea>\n        </div>\n\n        <div className=\"col-12 col-lg-3\">\n          <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n            Add Empleado\n          </button>\n        </div>\n        {error && (\n          <div className=\"col-12 my-3 bg-danger text-white p-3\">\n            Something went wrong...\n          </div>\n        )}\n      </form>\n    </div>\n  );\n};\n\nexport default EmpleadoForm;\n","C:\\Users\\DELL\\Desktop\\BootCamp\\ProyectosG\\Access-Control\\client\\src\\utils\\queries.js",[],"C:\\Users\\DELL\\Desktop\\BootCamp\\ProyectosG\\Access-Control\\client\\src\\utils\\mutations.js",[],"C:\\Users\\DELL\\Desktop\\BootCamp\\ProyectosG\\Access-Control\\client\\src\\components\\VisitaForm.js",[],"C:\\Users\\DELL\\Desktop\\BootCamp\\ProyectosG\\Access-Control\\client\\src\\utils\\helpers.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":21,"column":15,"nodeType":"57","messageId":"58","endLine":21,"endColumn":19},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]